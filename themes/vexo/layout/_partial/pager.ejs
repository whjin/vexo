<div class="guide-pager">
    <% if (page.prev) { %>
    <a href="<%- url_for(page.prev_link) %>"><span class="page-arrow">← </span>上一页</a>
    <% } else { %>
    <a class="unvisible" href="<%- url_for(page.prev_link) %>"><span class="page-arrow">←</span>上一页</a>
    <% } %>
    <% if (page.next) { %>
    <a href="<%- url_for(page.next_link) %>">下一页<span class="page-arrow"> →</span></a>
    <% } else { %>
    <a class="unvisible" href="<%- url_for(page.next_link) %>">下一页<span class="page-arrow">→</span></a>
    <% } %>
</div>